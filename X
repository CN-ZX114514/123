function Init()
    local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
end
function Test()
    print(Fluent.Options)
end
Init()
Test()
local Window = Fluent:CreateWindow({
    Title = "X脚本" .. Fluent.Version,
    SubTitle = "X完全免费",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 360),
    Acrylic = true, 
    Theme = "Amethyst",
    MinimizeKey = Enum.KeyCode.LeftControl
})
local Options = Fluent.Options
Fluent:Notify({
        Title = "X脚本",
        Content = "欢迎使用",
        SubContent = "欢迎UI", 
        Duration = 5
})
local Tabs = {
    Main = Window:AddTab({ Title = "主要", Icon = "boxes" }),
    Main = Window:AddTab({ Title = "破解", Icon = "code" }),
    Main = Window:AddTab({ Title = "外网", Icon = "code" }),
    Settings = Window:AddTab({ Title = "脚本", Icon = "code" }),
    Main = Window:AddTab({ Title = "其他", Icon = "code" }),
    Main = Window:AddTab({ Title = "FE", Icon = "code" }),
    Main = Window:AddTab({ Title = "翻译", Icon = "code" }),
    Main = Window:AddTab({ Title = "设置", Icon = "settings" }),
    Main = Window:AddTab({ Title = "插件(不保存)", Icon = "box" }),
}
Window:SelectTab(1)
